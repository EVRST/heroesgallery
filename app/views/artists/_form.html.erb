<%= simple_form_for(@artist) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :presentation_fr %>
    <%= f.input :presentation_en %>
    <%= f.input :presentation_nl %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
