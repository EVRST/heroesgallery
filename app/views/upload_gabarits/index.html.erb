<p id="notice"><%= notice %></p>

<h1>Listing Upload Gabarits</h1>

<table>
  <thead>
    <tr>
      <th>Upload</th>
      <th>Gabarit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_gabarits.each do |upload_gabarit| %>
      <tr>
        <td><%= upload_gabarit.upload_id %></td>
        <td><%= upload_gabarit.gabarit_id %></td>
        <td><%= link_to 'Show', upload_gabarit %></td>
        <td><%= link_to 'Edit', edit_upload_gabarit_path(upload_gabarit) %></td>
        <td><%= link_to 'Destroy', upload_gabarit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload gabarit', new_upload_gabarit_path %>
