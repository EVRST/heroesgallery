<% Gabarit.all.each do |gabarit| %>
    <div class="col-md-6">
            <%= simple_form_for(@upload_gabarit) do |f| %>
				  <%= f.error_notification %>

				  <div class="form-inputs">
				    <%= f.hidden_field :gabarit_id, value: gabarit.id %>
				    <%= f.hidden_field :upload_id, value: @upload.id %>
				  </div>

				  <div class="form-actions">
				    <%= f.submit gabarit.name, class: "btn btn-default btn-block" %><br>
				  </div>
				<% end %>
        </div>
<% end %>